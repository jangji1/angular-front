{
  "name": "ng-front",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "http://gitlab.home-learn.com:8824/middle/ng-front.git"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c=bs-config.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build:dev": "rm -rf ./aot/* && npm run set-config dev && node_modules/.bin/ngc -p tsconfig-aot.json &&  node_modules/.bin/rollup -c rollup-config.js && node ./copy-dist-files.js",
    "build:prod": "npm run set-config prod && node_modules/.bin/ngc -p tsconfig-aot.json &&  node_modules/.bin/rollup -c rollup-config.js && node ./copy-dist-files.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "copy-dist-files": "node ./copy-dist-files.js",
    "set-config": "node ./set-config.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~4.2.0",
    "@angular/common": "~4.2.0",
    "@angular/compiler": "~4.2.0",
    "@angular/compiler-cli": "~4.2.0",
    "@angular/core": "~4.2.0",
    "@angular/forms": "~4.2.0",
    "@angular/http": "~4.2.0",
    "@angular/platform-browser": "~4.2.0",
    "@angular/platform-browser-dynamic": "~4.2.0",
    "@angular/platform-server": "~4.2.0",
    "@angular/router": "~4.2.0",
    "@angular/tsc-wrapped": "~4.2.0",
    "@angular/upgrade": "~4.2.0",
    "@angular/material": "^2.0.0-beta.3",
    "angular-tree-component": "^3.2.3",
    "angular-in-memory-web-api": "~0.3.2",
    "angular2-uuid": "^1.1.1",
    "rxjs": "^5.1.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "jquery-ui-bundle": "^1.12.1",
    "ngx-pagination": "^3.0.0",
    "reflect-metadata": "^0.1.8",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/language-service": "^4.0.0",
    "@types/angular": "^1.5.16",
    "@types/angular-animate": "^1.5.5",
    "@types/angular-cookies": "^1.4.2",
    "@types/angular-mocks": "^1.5.5",
    "@types/angular-resource": "^1.5.6",
    "@types/angular-route": "^1.3.2",
    "@types/angular-sanitize": "^1.3.3",
    "@types/core-js": "0.9.36",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.36",
    "@types/jquery": "^2.0.44",
    "@types/jqueryui": "^1.11.32",
    "@types/node": "^6.0.45",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.2",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "source-map-explorer": "^1.3.3",
    "typescript": "^2.3.2"
  }
}
